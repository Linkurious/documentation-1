<div class="clearfix">
  <% members.forEach(function(member) { %>
    <div class='border-bottom' id='<%- slug(member.namespace) %>'>
      <div class="clearfix small pointer toggle-sibling">
        <div class="list-item">
            <a class='icon pin-right py1 dark-link caret-right'>â–¸</a>
            <span class='list-name name'><%= member.namespace.replace(/[#~]/, '.') %></span>
            <%= shortSignature(member).match(/\(.*\)$/, '')?.pop() %>
        </div>
      </div>
      <div class="clearfix display-none toggle-target">
        <%= renderSection({
          section: member,
          renderSection: renderSection,
          renderParamProperty: renderParamProperty,
     	    nested: true
        }) %>
      </div>
    </div>
  <% }) %>
</div>
