<tr>
  <td class='break-word'><span class='code bold'><%- property.name %></span> <code class='quiet'><%= formatType(property.type) %></code>
  <% if (property.default) { %>
    (default <code><%- property.default %></code>)
  <% } %></td>
  <td class='break-word'>
    <% if(property.description) { %>
      <span><%= md(property.description, true) %></span>
    <% } else { %>
      <span style="color: grey; font-style: italic;">No description.</span>
    <% } %>
  </td>
</tr>
<% if(property.properties && property.properties.length) { %>
  <% property.properties.forEach(function(childProperty) { %>
    <%= renderParamProperty({
      property: {...childProperty, name: `ㅤ${property._indent || ''}${childProperty.name}`, _indent: `ㅤ${property._indent || ''}`},
      renderParamProperty: renderParamProperty
    }) %>
  <% }) %>
<% } %>
